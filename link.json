{
  "name": "P22",
  "title": "Raspberry Pi Pico",
  "description": "Platform for Raspberry Pi Pico",
  "homepage": "http://www.OS-Q.com/P22",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P22.git"
  },
  "version": "1.0.8",
  "frameworks": {
      "baremetal": {
        "package": "E22A",
        "script": "extend/frameworks/sdk.py"
      },
      "arduino": {
        "package": "E22A",
        "script": "extend/frameworks/arduino.py"
      },
      "boot2": {
        "package": "E22A",
        "script": "extend/frameworks/boot2.py"
      }
  },
  "packages": {
    "E22A": {
        "type": "framework",
        "version": "https://github.com/OS-Q/E22A.git"
    },
    "toolchain-gccarmnoneeabi": {
        "type": "toolchain",
        "version": "=1.70201.0"
    },
    "tool-wizio-pico": {
        "type": "uploader",
        "version": "https://github.com/Wiz-IO/tool-wizio-pico.git"
    },
    "tool-pico-openocd": {
      "optional": true,
      "type": "debugger",
      "version": "https://github.com/Wiz-IO/tool-pico-openocd"
    }
  }
}
